# Wise API Configuration
# Get your API token from: Wise.com > Your Business Profile > Your Account > Integrations and Tools > API tokens

# Use 'sandbox' for testing or 'production' for real data
WISE_ENVIRONMENT=sandbox

# Your Wise API Token (Personal API Token for small business users)
WISE_API_TOKEN=your_api_token_here

# Your Wise Profile ID (found in your Wise account)
WISE_PROFILE_ID=your_profile_id_here

# Server Configuration
PORT=3000

# Development Mode (for UI testing without API calls)
# Set to 'true' to use mock data instead of Wise API
MOCK_MODE=false
